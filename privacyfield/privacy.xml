<?xml version="1.0" encoding="utf-8"?>
<form>
  <fields name="image_privacy">
    <fieldset name="privacy"
      label="Image Privacy"
    >
      <field
        name="js_privacy"
        type="list"
        default="40"
        label="Privacy"
        description="The space this image belongs to:">
        <option value="0">Public</option>
        <option value="20">Site Members</option>
        <option value="30">Friends</option>
        <option value="40">Only me</option>
      </field>
     </fieldset>
  </fields>
</form>